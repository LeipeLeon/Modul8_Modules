	<string># A simple path editor - Yves @ GarageCube

import math

pointList = []
tool = 'add'
smoothAmount = 0.5
mouseDragPt = None
mouseDragPtSmooth = False
smoothedOnly = False
closed = False
renderedPath = []
playMode = False
playTimePos = 0.0
playCurveIndex = 0
playCurrentAngle = 0
selection = None
showSpeeds = False

def addRenderPoint(nx,ny,speed):
	global renderedPath
	rp = {'X2':nx,'Y2':ny,'pos2':0,'speed2':speed}
	if len(renderedPath)!=0:
		lp = len(renderedPath)-1
		x1 = renderedPath[lp]['X2']
		y1 = renderedPath[lp]['Y2']
		pos1 = renderedPath[lp]['pos2']
		speed1 = renderedPath[lp]['speed2']
		rp['X1'] = x1
		rp['Y1'] = y1
		rp['pos1'] = pos1
		rp['speed1'] = speed1
		vx = nx-x1
		vy = ny-y1
		rp['pos2'] = pos1+math.sqrt(vx*vx + vy*vy)
	renderedPath.insert( len(renderedPath),rp)

def drawCurve():
	global renderedPath
	global selection
	renderedPath = []
	circleSize = 10
	r = 0.5
	g = 0.7
	b = 1
	a = 1

	scircleSize = 2
	sr = 0.7
	sg = 1
	sb = 0.3

	ser = 1
	seg = 0.7
	seb = 0.3

	module.clearDrawings('path')

	# Draw the path
	ox= None
	oy =None
	if not smoothedOnly:
		for pt in pointList:
			x = pt['X']
			y = pt['Y']
			if pt==selection:
				module.paintOval('path',x-circleSize*0.5,y-circleSize*0.5, circleSize, circleSize,ser,seg,seb,a)
			else:
				module.paintOval('path',x-circleSize*0.5,y-circleSize*0.5, circleSize, circleSize,r,g,b,a)
			if ox!=None:
				module.drawLine('path',ox,oy, x, y, 1,r,g,b,a)
			
			if showSpeeds:
				module.drawLine('path',x,y+circleSize, x, y+circleSize+pt['speed']*15, 4,r,g,b,0.5)
			
			ox = x
			oy = y
		if closed:
			if ox!=None and len(pointList)&gt;2:
				pt = pointList[0]
				x = pt['X']
				y = pt['Y']
				module.drawLine('path',ox,oy, x, y, 1,r,g,b,a)

	# Draw the smooth info
	if smoothAmount !=0 and len(pointList)&gt;=3:
		pti = 0						
		ntoprocess = len(pointList)
		if not closed:
			ntoprocess -= 1						
		for pti in range(0, ntoprocess):

			if not closed and pti==len(pointList)-2:
				pointList[pti]['NX']  = pointList[pti+1]['X']
				pointList[pti]['NY']  = pointList[pti+1]['Y']
				pointList[pti]['nspeed'] = pointList[pti+1]['speed']
				continue
			
			pti1 = pti +1
			if pti1&gt;=len(pointList):
				pti1 -= len(pointList)

			pti2 = pti +2
			if pti2&gt;=len(pointList):
				pti2 -= len(pointList)

			x1 = pointList[pti]['X']
			y1 = pointList[pti]['Y']
			x2 = pointList[pti1]['X']
			y2 = pointList[pti1]['Y']
			x3 = pointList[pti2]['X']
			y3 = pointList[pti2]['Y']
			
			pointList[pti]['NX'] = x2	# Store next point for easy access
			pointList[pti]['NY'] = y2
			pointList[pti]['nspeed'] = pointList[pti1]['speed']

			v1x = x1-x2
			v1y = y1-y2			
			v2x = x3-x2
			v2y = y3-y2
			m1 = math.sqrt(v1x*v1x + v1y*v1y)
			m2 = math.sqrt(v2x*v2x + v2y*v2y)
			if m1&lt;m2:
				x3 = x2 + (v2x/m2) * m1
				y3 = y2 + (v2y/m2) * m1
			else:
				x1 = x2 + (v1x/m1) * m2
				y1 = y2 + (v1y/m1) * m2

			x = (x3+x1)*0.5
			y = (y3+y1)*0.5
			x = (x*smoothAmount) + (x2*(1.0-smoothAmount))
			y = (y*smoothAmount) + (y2*(1.0-smoothAmount))
			
			x = x + pointList[pti]['SVX']*smoothAmount
			y = y + pointList[pti]['SVY']*smoothAmount
						
			pointList[pti]['SX'] = x # Store attraction point
			pointList[pti]['SY'] = y 
			
			module.paintOval('path',x-circleSize*0.5,y-circleSize*0.5, circleSize, circleSize,sr,sg,sb,0.5)

		ox= None
		oy =None
		ntoprocess = len(pointList)
		
		#print 'start curve'

		for pti in range(0, ntoprocess):
			pt = pointList[pti]
			
			if not closed and pti ==len(pointList)-1:
				continue 
			
			if not 'NX' in pt:
				continue
			
			if ox==None:
				if closed:
					bx = pointList[len(pointList)-1]['SX']
					by = pointList[len(pointList)-1]['SY']
				else:
					bx = pt['X']
					by = pt['Y']
			else:
				bx = ox
				by = oy

			nx = pt['NX'] 
			ny = pt['NY'] 

			if not closed and pti ==len(pointList)-2:
				sx = nx
				sy = ny
			elif 'SX' in pt:
				sx = pt['SX']
				sy = pt['SY']
			else:
				sx = nx
				sy = ny
								
			ox = sx
			oy = sy

			v1x = bx-sx
			v1y = by-sy			
			m1 = math.sqrt(v1x*v1x + v1y*v1y)
			nsegs = m1/5
			speed1 = pt['speed']
			speed2 = pt['nspeed']
									
			for i in range(0,nsegs):
				f = i/nsegs
				x2 = nx + (sx-nx) *f
				y2 = ny + (sy-ny) *f
				
				x = bx + (x2-bx)*f
				y = by + (y2-by)*f
				module.paintOval('path',x-scircleSize*0.5,y-scircleSize*0.5, scircleSize, scircleSize,sr,sg,sb,1.0)
				addRenderPoint(x,y,speed1+ (speed2-speed1)*f )

	else:
		# For not smoothed curve I still need to render the curve... But it is easier
		for pt in pointList:
			x = pt['X']
			y = pt['Y']
			addRenderPoint(x,y,pt['speed'])

	if len(renderedPath)!=0:
		if closed:
			addRenderPoint(renderedPath[0]['X2'],renderedPath[0]['Y2'], renderedPath[0]['speed2'])
		del renderedPath[0]	# dont need the first point

	module.finishDrawings('path')


def distanceToLine (x1, y1, x2,  y2,  px, py):

	v1x = px-x1
	v1y = py-y1

	v2x = x2-x1
	v2y = y2-y1

	temp = math.sqrt ( (v2x * v2x) + (v2y * v2y) )

	v2x = v2x/temp	
	v2y = v2y/temp

	dot_product = (v1x * v2x) + (v1y * v2y)

	if dot_product&lt;=0:
		vtx = x1
		vty = y1
		dx = px-vtx
		dy = py-vty
		return math.sqrt ((dx*dx)+(dy*dy))
	elif dot_product&gt;=temp:
		vtx = x2
		vty = y2
		dx = px-vtx
		dy = py-vty
		return math.sqrt ((dx*dx)+(dy*dy))
	else:
		v3x = v2x * dot_product;
		v3y = v2y * dot_product;

		vtx = x1 + v3x;
		vty = y1 + v3y;

		dx = px-vtx;
		dy = py-vty;

		return math.sqrt ((dx*dx)+(dy*dy))

drawCurve()

