	<string>
## polygons around the center...
## vj //c - 01.2015 v0.1 (beta release)

from random import randint, random, seed
from math import sin, cos, radians

## this is from 120 eq paint circle, so there's probably some un-needed code in here..


## create a dictionary for the variables
eqDef = {
'active' : False,				## are we live?
}

seed()

##
## if you're looking for the size, the center x and y are spefied in the call to drawShapes
## the size of the canvas can be changed by going to 'Visual' -&gt; 'Tools' -&gt; 'Show All' and change the sizes of draw0 and draw1 (current @ 640x480)


## we will have two pages, draw0 and draw1 
## we can switch back and forth with 
##    drawPageNum =  not drawPage num
##    myDraw = 'draw%d' %int(drawPageNum)
## to have a 'cleaner' looking animation 

drawPageNum = False
drawNow = False
numPages = 0				# counter for how many pages have been drawn, mainly for debugging...

waitTime = 15				# if the animation is skipping frames, increase the wait between displaying draw0/draw1
						# lower value draws faster but might skip frames...
currWaitTime = waitTime

## init the drawing layers
module.setDrawingsLayer('shapesLayer','draw0')
module.setDrawingsLayer('shapesLayer','draw1')

## keep a stack of shapes drawn
shapeStack = []
##shapeStack.append([0,0,0,0,[0,0,0]])

maxStack = 5		## maximum number of polygons to keep

## output limies

outE = True		## allow even number shapes (outer)
outO	= False 		## allow odd number of shapes (outer)

def isodd(num):
	return num &amp; 1 and True or False

def clearPage(myDrawPage) :	
	
	print "clearing page :", myDrawPage
	module.clearDrawings('draw0')
	module.finishDrawings('draw0')
	module.sendContentToLayer('draw0', '120 polygons', 1)
	module.clearDrawings('draw1')
	module.finishDrawings('draw1')
	module.sendContentToLayer('draw1', '120 polygons', 1)
	numPages = 0
	shapeStack[:] = []



##
##
##  the 'special sauce' of the module...

def drawShapes(page, xCenter, yCenter, bigR, bigPoints, smR, smSides, smThick, r,g,b) :
	#
	# page 		- name of canvas we're drawing on 
	# x/y Center	- center of screen (320x240 for 640x480, etcâ€¦)
	# bigR		- how far from the center to draw the polygons
	# bigPoints	- how many polygons to draw
	# smR		- "radius" of polygon
	# smSides	- number of sides on polygon
	# smThick		- thickness of the pen

	if bigPoints &gt; 0 :
		for points in range(0, bigPoints ) :
			myDegrees = 360.0 * points / bigPoints	## so in python if you want a float, one item has to  be a float.
			myRadians = radians(myDegrees)		## python uses radians, not degrees.  only lost an hour figuring that out  

			xSmall = xCenter + cos(myRadians) * bigR	##  find 'x' of center point for smShape
			ySmall = yCenter  + sin(myRadians)  * bigR	##  find 'y' of center point for smShape

			### this code creates an evolution of the polygon in many stages
			myOffset = 0
			if myDegrees &lt; 180 :
				myOffset = myDegrees + 180
			else :
				myOffset = myDegrees - 180
					
			### if you change the above lines to += and -=  it will change the order of the lines being drawn, sort of creates stars/spirographs...
#				if myDegrees &lt; 180 :		
#					smDegrees += 180
#				else :
#					smDegrees -= 180
			
									
			for ss in range (0, smSides ) :				## now draw the smaller shape
				smDegrees = 360.0 * ss/smSides		## yeah.. need a float value there
			
				smRadians = radians(smDegrees+myOffset)
				xStart = xSmall + cos(smRadians) * smR
				yStart = ySmall + sin(smRadians) *smR
				
				if (ss + 1) == smSides :
					myNextPoint = 0
				else :
					myNextPoint = ss + 1
				
				### note to self - the diversity in the potential output is based on how the next angle (and subsequent x,y) is calculated
				smDegreesNext  = (360.0 * myNextPoint  / smSides ) + myOffset
				smRadiansNext  = radians(smDegreesNext)
				xEnd = xSmall + cos(smRadiansNext) * smR
				yEnd = ySmall + sin(smRadiansNext)  * smR
				
				## the money shot..
				module.drawLine(page, xStart, yStart, xEnd, yEnd, smThick, r, g, b, 1.0)


def drawRandom(draw) :
		global drawPageNum,numPages
		## drawShapes('draw0', 320, 240, 80, 8, 40, 6, 2)
		myBigRadius = randint(50, 100)
		myNumShapes = randint(2,8)
		if isodd(myNumShapes) :
			if not outO :
				myNumShapes -= 1
		else :
			if not outE :
				myNumShapes += 1
		mySmRadius = randint(20,80)
		mySmSides = randint(3, 10)
		myRndColor = [random(), random(), random()]

		if (len(shapeStack) == maxStack ) :
			shapeStack.pop(0)			
		shapeStack.append([myBigRadius ,myNumShapes, mySmRadius, mySmSides,myRndColor,numPages] )
		
		removeItems = []
		## draw the 0..maxStack shapes...
		
		myDrawPage = 'draw%s' %int(drawPageNum)	##draw0 or dra
		module.clearDrawings(myDrawPage)
		for i in range (len(shapeStack)) :
			##			     bigRadius,            numShapes                 smRaidus                 mySmSides                                	
			if (i &lt; len(shapeStack) ) :
#				myBoundry = shapeStack[i][0] - shapeStack[i][2]    ## if this number exceeds the width of the screen, we can remove the data from the stack
#
#				if  ( myBoundry &gt; 320 ) :       ### shape is outside of viewArea
#					removeItems.insert(0,i)		## create a list of items that should be removed, remove them after walking through the list...
#				else :

					drawShapes(myDrawPage, 320, 240, shapeStack[i][0],shapeStack[i][1],shapeStack[i][2],shapeStack[i][3],3, shapeStack[i][4][0], shapeStack[i][4][1], shapeStack[i][4][2])

					## now, modulate some variables
					shapeStack[i][0] = int (shapeStack[i][0] *1.5 )              # grow the image ?
					shapeStack[i][2] = int (shapeStack[i][2] *1.5 )              # grow the image ?

					## dim the colors....
#					shapeStack[i][4][0] *= 0.9
#					shapeStack[i][4][1] *= 0.9
#					shapeStack[i][4][2] *= 0.9
			
			
			## uncomment the line below to print the page num on the image so we can capture output and see if we're dropping frames...
#				module.drawString(myDrawPage,'%s' %str(numPages) , 30, 20, 1, 1, 1, 1.0)
		
		for x in range(len(removeItems)) :
			shapeStack.pop(x)

		numPages +=1

## draw some shapes...
def drawInColor(draw,numShades) :
		## drawShapes('draw0', 320, 240, 80, 8, 40, 6, 2)
		mainColor  = 1.0
		shadeDown = mainColor / numShades
		for i in range(0,numShades) :
			myBigRadius = randint(30, 300)
			myNumShapes = randint(2,8)
			mySmRadius = randint(50,100)
			mySmSides = randint(3, 20)
			myRndColor = [random(), random(), random()]

			#print myNumShapes, "  ",myBigRadius, "  --  ",  mySmSides, "   ", mySmRadius

			drawShapes(draw, 320, 240, myBigRadius, myNumShapes, mySmRadius, mySmSides, randint(2,3), mainColor,0,0)
			mainColor -= shadeDown
			
		module.finishDrawings(draw)
		module.sendContentToLayer(draw , 'hi', 1)		</string>
	<key>SCRIPT_KEYWORDEVENT</key>
	<string></string>
	<key>SCRIPT_MESSAGE</key>
	<string>#print msg, param
param = param['value']  	## because i get tired of typing if param['value'] == 1

global eqDef, drawPageNum,outE,outO

myDrawPage = 'draw%s' %int(drawPageNum)

## i usually set up my commands in a standard way  variblenameVale as one message
## and truncate the last few bits to do the change

if msg == 'eqActive' :
	eqDef['active'] = param
elif msg == 'clear' and param :
	clearPage(myDrawPage)
	myTrace = modul8.getValue('ctrl_master_traceLevel', 0)
	modul8.setValue('ctrl_master_traceLevel', 0, 0)
	modul8.setValue('ctrl_master_traceLevel' , myTrace, 0 )
elif msg == 'random' and param :
	drawRandom(myDrawPage)
	module.finishDrawings(myDrawPage)
	module.sendContentToLayer(myDrawPage, 'hi', 1)
elif msg == 'shades' and param :
	drawInColor('draw0', 4)
elif msg == 'outE' :
	outE = param
elif msg == 'outO' :
	outO = param
</string>
	<key>SCRIPT_PAUSEEVENT</key>
	<string></string>
	<key>SCRIPT_PERIODICAL</key>
	<string>global eqDef, drawPageNum, drawNow,currWaitTime,waitTime

if eqDef['active'] :
	##bands = modul8.getSoundBands()

## since modul8 is deterministic and everything happens as fast as possible in here, keep a flag to know when it's time to redraw
	if drawNow :
		myDrawPage = 'draw%s' %int(drawPageNum)	##draw0 or draw1
		
		if currWaitTime &gt; 0 :
			#print "      wait ", currWaitTime
			currWaitTime -= 1
		else :
			module.sendContentToLayer(myDrawPage, 'hi', 1)
			currWaitTime = waitTime
			drawNow = False
			#print "drawing ", numPages
	## use two canvases to prevent clipping while drawing...
	else :
		drawPageNum = not drawPageNum
		myDrawPage = 'draw%s' %int(drawPageNum)	##draw0 or draw1
		module.clearDrawings(myDrawPage)
		drawRandom(myDrawPage) 
		module.finishDrawings(myDrawPage)
		drawNow = True							## we're done updating the canvas, on the next
												## go around, show it!
