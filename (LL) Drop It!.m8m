print speedDict

def flash(state):
	checkSharedDict()
	if state:	# BtnDown
		if module.getValue('useGlobalColor', 0):
			modul8.setValue('ctrl_master_addColorR',sharedDict['currentColor'][0],0)
			modul8.setValue('ctrl_master_addColorG',sharedDict['currentColor'][1],0)
			modul8.setValue('ctrl_master_addColorB',sharedDict['currentColor'][2],0)
		else:
			modul8.setValue('ctrl_master_addColorR',1,0)
			modul8.setValue('ctrl_master_addColorG',1,0)
			modul8.setValue('ctrl_master_addColorB',1,0)
	else:
		modul8.setValue('ctrl_master_addColorR',0,0)
		modul8.setValue('ctrl_master_addColorG',0,0)
		modul8.setValue('ctrl_master_addColorB',0,0)

def invert(state):
	checkSharedDict()
	if state:	# BtnDown
		if module.getValue('useGlobalColor', 0):
			modul8.setValue('ctrl_master_invColorR',sharedDict['currentColor'][0],0)
			modul8.setValue('ctrl_master_invColorG',sharedDict['currentColor'][1],0)
			modul8.setValue('ctrl_master_invColorB',sharedDict['currentColor'][2],0)
		else:
			modul8.setValue('ctrl_master_invColorR',1,0)
			modul8.setValue('ctrl_master_invColorG',1,0)
			modul8.setValue('ctrl_master_invColorB',1,0)
	else:
		modul8.setValue('ctrl_master_invColorR',0,0)
		modul8.setValue('ctrl_master_invColorG',0,0)
		modul8.setValue('ctrl_master_invColorB',0,0)
	
def checkSharedDict():
	if 'currentColor' in sharedDict:
		module.setAttribute('useGlobalColor', 'HIDDEN', 0)
		print 'useGlobalColor: ', module.getValue('useGlobalColor', 0), 
	else:
