'direct_globalMask_surfaceBlend_Src':'str',\
'direct_globalMask_surfaceBlend_Dest':'str',\
'direct_layer_text_field':'str',\
'set_direct_openProject':'str'\
}

def autoTransformValue (key,value) :
	
	global KeywordNeedInt, KeywordNeedStrUnicode
	
	if key in KeywordNeedInt :
		return int (value)
	elif key in KeywordNeedStrUnicode:
		return unicode (value)
	else :
		return float (value)
