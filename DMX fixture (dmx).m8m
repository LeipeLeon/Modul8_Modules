	<key>SCRIPT_MESSAGE</key>
	<string>#print param

#Select fixture
if msg == 'fixtures' :
	
	fixtureSelection = param ['selection']
	
	#estetic on/off
	if fixtureSelection.startswith ('|| ') :
		fixtureSelection = fixtureSelection[3:]
	#
	
	UpdateUIFixtureSelection()
	keywordSelectionIndex = -1
	UpdateUIKeywordSelection()

#Add fixture	
elif msg == 'addFixture' and param['value'] :
	
	fixName = 'new fixture'
	inc = 1
	
	while fixName + ' ' + str (inc) in fixturesDict :
		inc += 1
	fixName = fixName + ' ' + str (inc)
	
	fixtureSelection = fixName
	fixturesDict[fixName] = {'UNIVERSE':0,'STARTCHANNEL':1,'KEYWORDS':[],'ON_OFF':1}
	
	#Update UI
	UpdateUIFixture()
	UpdateUIFixtureSelection()
	
	#No Keyword selection
	keywordSelectionIndex = -1
	UpdateUIKeywordSelection()

#Remove fixture
elif msg == 'removeFixture' and not param['value'] :
	
	if fixtureSelection != '' and fixtureSelection in fixturesDict :
		del fixturesDict[fixtureSelection]
		fixtureSelection = ''
		
		
		#Update UI
		UpdateUIFixture()
		UpdateUIFixtureSelection()
		
		#No keyword selection
		keywordSelectionIndex = -1
		UpdateUIKeywordSelection()
		

#Copy fixture
elif msg == 'copyFixture' and param['value'] :
	
	if fixtureSelection != '' and fixtureSelection in fixturesDict :
	
		fixName = fixtureSelection
		inc = 1
		
		while fixName + ' ' + str (inc) in fixturesDict :
			inc += 1
		fixName = fixName + ' ' + str (inc)
		
		fixturesDict[fixName] = deepcopy (fixturesDict[fixtureSelection])
		
		#fixtureSelection = fixName
		
		#Update UI
		UpdateUIFixture()
		#UpdateUIFixtureSelection()
			
						
	
#edit
####
#Rename the fixture
elif msg == 'fixtureName' :
	
	if fixtureSelection != '' and fixtureSelection in fixturesDict :
		
		if not param['text'] in fixturesDict and param['text'] != '' :
			#Change the name if it does not exist
			fixturesDict [param['text']] = fixturesDict[fixtureSelection]
			del fixturesDict[fixtureSelection]
			fixtureSelection = param['text']

		#Update the UI 	
		UpdateUIFixture()
		UpdateUIFixtureSelection()		
					
#Change the channel
elif msg == 'fixtureChannel' :
	if fixtureSelection != '' and fixtureSelection in fixturesDict :
		fixturesDict[fixtureSelection]['STARTCHANNEL'] = int(param['value'])
		
		#Update the UI
		UpdateUIKeywordSelection()
		
#Fixture ON/OFF

elif msg == 'fixtureOnOff' :
	if fixtureSelection != '' and fixtureSelection in fixturesDict :
		fixturesDict[fixtureSelection]['ON_OFF'] = int(param['value'])
		
		#Update the UI
		UpdateUIKeywordSelection()
		
		#For estetic on/off in the list
		UpdateUIFixture()
				

#Change the layer position
elif msg == 'fixtureLayerPos' :
	
	if fixtureSelection != '' and fixtureSelection in fixturesDict and keywordSelectionIndex != -1 :
		fixturesDict[fixtureSelection]['KEYWORDS'][keywordSelectionIndex]['LAYER_POSITION'] = int(param['value'])
		
	#Update UI
	UpdateUIKeywordSelection()
	
#Change the value multiplicator (val mul)
elif msg == 'valMul' :
	
	if fixtureSelection != '' and fixtureSelection in fixturesDict and keywordSelectionIndex != -1 :
		fixturesDict[fixtureSelection]['KEYWORDS'][keywordSelectionIndex]['VAL_MUL'] = param['value']
		
	#Update UI
	UpdateUIKeywordSelection()

	

		

#KEYWORDS

elif msg == 'addKeyword' and param['value'] :
	
	#prend la position de layer du keyword selectionné
	if keywordSelectionIndex != -1 :
		
		NewLayerPosition = fixturesDict[fixtureSelection]['KEYWORDS'][keywordSelectionIndex]['LAYER_POSITION'] 
	else :
		NewLayerPosition = 0
	#
	
	#prend le keyword du keyword selectionné
	if keywordSelectionIndex != -1 :
		
		NewKeywordName = fixturesDict[fixtureSelection]['KEYWORDS'][keywordSelectionIndex]['KEYWORD_NAME'] 
	else :
		NewKeywordName = 'new keyword'
	#

	
	
	fixturesDict[fixtureSelection]['KEYWORDS'].append ({'KEYWORD_NAME': NewKeywordName,'LAYER_POSITION': NewLayerPosition,'VAL_MUL':1})
	
	
	keywordSelectionIndex = (len(fixturesDict[fixtureSelection]['KEYWORDS'])-1)
	
	#Update UI
	UpdateUIKeywordSelection()

elif msg == 'removeKeyword' and not param['value'] : #PROB SI NO KEYWORD SELECTION
	
	if fixtureSelection != '' and fixtureSelection in fixturesDict and keywordSelectionIndex != -1 :	
		#index = int (module.getValue('keywords','selection'))
		del fixturesDict[fixtureSelection]['KEYWORDS'][keywordSelectionIndex]
		
		#keyword selection
		keywordSelectionIndex -= 1
		if keywordSelectionIndex &lt;0 and len (fixturesDict[fixtureSelection]['KEYWORDS']) &gt; 0 :  keywordSelectionIndex = 0
		
		#Update UI
		UpdateUIKeywordSelection()
		
elif msg == 'upKeyword' and param['value'] : 
	
	if fixtureSelection != '' and fixtureSelection in fixturesDict and keywordSelectionIndex != -1 :
		
		keywordToMove = fixturesDict[fixtureSelection]['KEYWORDS'].pop(keywordSelectionIndex)
		
						
		keywordSelectionIndex -= 1
		if keywordSelectionIndex &lt; 0 : keywordSelectionIndex = 0
		
		fixturesDict[fixtureSelection]['KEYWORDS'].insert(keywordSelectionIndex, keywordToMove)
		
		UpdateUIKeywordSelection()
		
elif msg == 'downKeyword' and param['value'] : 
	
		keywordToMove = fixturesDict[fixtureSelection]['KEYWORDS'].pop(keywordSelectionIndex)
		
		keywordSelectionIndex += 1
		if keywordSelectionIndex &gt; len (fixturesDict[fixtureSelection]['KEYWORDS']) : 			keywordSelectionIndex = len (fixturesDict[fixtureSelection]['KEYWORDS'])
		
		fixturesDict[fixtureSelection]['KEYWORDS'].insert(keywordSelectionIndex, keywordToMove)
		
		UpdateUIKeywordSelection()

	
	
		
elif msg == 'keywords' :
			
	if fixtureSelection != '' and fixtureSelection in fixturesDict :
				
		keywordSelectionIndex = int (param ['selection'])
		
		#Update UI
		UpdateUIKeywordSelection()
		
elif msg == 'keywordfield' : 
		
	if fixtureSelection != '' and fixtureSelection in fixturesDict :
		
		if keywordSelectionIndex != -1 :
					
			fixturesDict[fixtureSelection]['KEYWORDS'][keywordSelectionIndex]['KEYWORD_NAME'] = param ['text']
		
		#Update UI
		UpdateUIKeywordSelection()
		
elif msg=='pickKeyword':
	
	pickMode = param['value']
	
elif msg =='log' :
	log = param['value']
	
		
		
