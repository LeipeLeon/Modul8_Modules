	<key>SCRIPT_INIT</key>
	<string>#
if modul8.getValue ('direct_version',0) &lt; 2.6 : module.stop() #not working before the v2.6
#

from copy import deepcopy

pickMode = 0
log = 0

if  module.getDefaults() == {} :
	fixturesDict = {'new fixture':{'UNIVERSE':0,'STARTCHANNEL':1,'KEYWORDS':[],'ON_OFF':1}}
else :
	fixturesDict = module.getDefaults()
	

#List of fixtures from the fixturesDictionary
listOfFixtures = []
	
for fixtureName in fixturesDict :
	listOfFixtures.append (fixtureName)

#Selection
fixtureSelection = ''
keywordSelectionIndex = -1

def UpdateUIFixture () :
	
	#Update the list of fixture
	listOfFixtures = []
	
	for fixtureName in fixturesDict :
		
		#estetic ON /OFF
		if not fixturesDict[fixtureName]['ON_OFF'] :
			fixtureName = '|| ' + fixtureName
		#
			
		listOfFixtures.append (fixtureName)
	
	#sort the listOfFixtures
	listOfFixtures.sort()
	
	#Update the UI	
	module.setAttribute('fixtures','TEXTLIST', listOfFixtures)
		
	#Update the UI selection
	module.setValue ('fixtures', 0, fixtureSelection)
	
	
		
		
def UpdateUIFixtureSelection () :
	
	
	if fixtureSelection != '' and fixtureSelection in fixturesDict :
		
		module.showGroup('fixtureParam')	
		
		module.setValue('fixtureName',0, fixtureSelection)
		module.setValue('startChannel',0, fixturesDict[fixtureSelection]['STARTCHANNEL'])
		module.setValue('fixtureOnOff',0, fixturesDict[fixtureSelection]['ON_OFF'])
			
	else :
		module.hideGroup('fixtureParam')
		module.hideGroup('keywordParam')
		
def UpdateUIKeywordSelection ():
      
	if keywordSelectionIndex != -1 :
		
		keywordSelection = fixturesDict[fixtureSelection]['KEYWORDS'][keywordSelectionIndex]['KEYWORD_NAME']
		keywordSelectionLayerPosition = fixturesDict[fixtureSelection]['KEYWORDS'][keywordSelectionIndex]['LAYER_POSITION']
		keywordSelectionMulVal = fixturesDict[fixtureSelection]['KEYWORDS'][keywordSelectionIndex]['VAL_MUL']
		
		#update selection focus
		module.setValue ('keywords',0, keywordSelectionIndex)
		
		#update keyword field
		module.setValue ('keywordfield',0, keywordSelection)
		
		#update layer pos field
		module.setValue ('layerPos',0, keywordSelectionLayerPosition)
		
		#update value multiplicator (val mul)
		module.setValue ('valMul',0, keywordSelectionMulVal)
		
		#show keyword param
		module.showGroup('keywordParam')	
		
	else :
		module.setValue ('keywords',0,-1)
		module.setValue ('keywordfield',0,'')
		
		#hide keyword param
		module.hideGroup('keywordParam')	
		
		
	if fixtureSelection != '' and fixtureSelection in fixturesDict :
		#update list of keyword
		channelKeywordList = []
		startChannel = int (fixturesDict[fixtureSelection]['STARTCHANNEL'])
		for index,keyword in enumerate (fixturesDict[fixtureSelection]['KEYWORDS']) :
			
			chNum = index + startChannel
			if chNum &gt; 512 : chNum = 512
			
			indexKey = 'ch ' + str (chNum) + '/pos '+ str(keyword['LAYER_POSITION']) +' - '+ keyword['KEYWORD_NAME']
			channelKeywordList.append (indexKey)
			
		module.setAttribute('keywords','TEXTLIST', channelKeywordList)
			
		
	
def SendDmx () :
	
	pass
	
	
UpdateUIFixture()
UpdateUIFixtureSelection ()</string>
