	<string>if param['NAME'][0:4] == 'drop':
	if param['value'] &lt; 0.0:
		isFlashing = False # NOOP
	elif param['value'] == 0.0:
		resetFlash()
	elif param['value'] &gt; 0.0:
		doFlash(int(param['value']))
		
elif param['NAME'] == 'stepper' and param['value'] != 0.0:
	print speedDictKey, int(param['value'])

	module.setValue('drop'+str(speedDictKey), 0, 0)
	speedDictKey = speedDictKey + int(param['value'])
	if speedDictKey == 8 or speedDictKey == -1:
		speedDictKey = 0
		resetFlash()
	else:
		doFlash(speedDictKey)
	module.setValue('drop'+str(speedDictKey), 0, 1)
	print speedDictKey
	

elif msg == 'flashType':
	flashType = param['value']
else:
	print msg, param
	
