	<key>SCRIPT_PERIODICAL</key>
	<string>if isFlashing:
	if shouldStartFlash() and not inFlash:
		inFlash = 1

if inFlash:
	if inFlashCounter &lt; module.getValue('duration',0)/fps:
		if flashType:
			flash(1)
		else:
			invert(1)
		inFlashCounter += elapsed
	else:
		flash(0)
		invert(0)
		inFlashCounter = 0
		inFlash = 0

# Speed is aprox. 60fps
fpsTime += elapsed
fpsCount += 1
if fpsTime &gt; 1:
	module.setAttribute('fps', 'CAPTION', str(fpsCount)+' fps')
	fps = fpsCount
	fpsCount = 0
	fpsTime = 0.0
