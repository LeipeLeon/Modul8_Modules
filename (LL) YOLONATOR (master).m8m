	<string># Â© 2015 Leon Berenschot (VJ LeipeLeon)
# Version 1.0 2015-05-25
#    initial release

sharedDict = module.getSharedDictionary()

isFlashing = False
flashType = 1

previousBlinkNumber = -1
#speedDict = {'1':128, '11':64, '12':32, '14':16, '18':8, '116':4, '132':2,'0': 0}
speedDict = [0,128, 64, 32, 16, 8, 4, 2]
speedDictKey = 0
currentColor = [1,1,1]
offset = 0
inFlash = 0
inFlashCounter = 0
fps = 60.0
fpsTime = 0.0
fpsCount = 0
buttonBeatPosition = 0

module.setAttribute('useGlobalColor', 'HIDDEN', 1)

def shouldStartFlash():
	currentBeatPosition = sharedDict['BMP_POSITION']
	offset = 0
	if module.getValue('quantize',0):
		pos = int( currentBeatPosition * 16)
	else:
		pos2 = int( buttonBeatPosition * 16 )
		offset = pos2%speedDict[speedDictKey]		
		pos = int( currentBeatPosition * 16 - offset)
	blinkNumber = pos%speedDict[speedDictKey]
	return blinkNumber == 0


def resetColor():
	modul8.setValue('ctrl_master_addColorR',0,0)
	modul8.setValue('ctrl_master_addColorG',0,0)
	modul8.setValue('ctrl_master_addColorB',0,0)
	modul8.setValue('ctrl_master_invColorR',0,0)
	modul8.setValue('ctrl_master_invColorG',0,0)
	modul8.setValue('ctrl_master_invColorB',0,0)

def flash(state):
	checkSharedDict()
	if state:	# BtnDown
		if module.getValue('useGlobalColor', 0):
			modul8.setValue('ctrl_master_addColorR',sharedDict['currentColor'][0],0)
			modul8.setValue('ctrl_master_addColorG',sharedDict['currentColor'][1],0)
			modul8.setValue('ctrl_master_addColorB',sharedDict['currentColor'][2],0)
		else:
			modul8.setValue('ctrl_master_addColorR',1,0)
			modul8.setValue('ctrl_master_addColorG',1,0)
			modul8.setValue('ctrl_master_addColorB',1,0)
	else:
		resetColor()

def invert(state):
	checkSharedDict()
	if state:	# BtnDown
		if module.getValue('useGlobalColor', 0):
			modul8.setValue('ctrl_master_invColorR',sharedDict['currentColor'][0],0)
			modul8.setValue('ctrl_master_invColorG',sharedDict['currentColor'][1],0)
			modul8.setValue('ctrl_master_invColorB',sharedDict['currentColor'][2],0)
		else:
			modul8.setValue('ctrl_master_invColorR',1,0)
			modul8.setValue('ctrl_master_invColorG',1,0)
			modul8.setValue('ctrl_master_invColorB',1,0)
	else:
		resetColor()

def doFlash(speed):
	global buttonBeatPosition, speedDictKey, isFlashing, inFlashCounter, inFlash
	buttonBeatPosition = sharedDict['BMP_POSITION']
	speedDictKey = speed
	isFlashing = True
	if not module.getValue('quantize',0):
		inFlashCounter = 0
		inFlash = 1
		
def resetFlash():
	global isFlashing
	isFlashing = False
	flash(0)
	invert(0)						
		
def checkSharedDict():
	if 'currentColor' in sharedDict:
		module.setAttribute('useGlobalColor', 'HIDDEN', 0)
	else:
		module.setAttribute('useGlobalColor', 'HIDDEN', 1)

checkSharedDict()
